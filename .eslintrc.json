{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "process": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "plugins": ["babel"],
    "parser": "babel-eslint",
    "rules": {
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "semi": ["error", "always"],
      "react/prop-types": 0,
      "comma-dangle": ["error", "always-multiline"],
      "no-invalid-this": 0,
      "babel/no-invalid-this": 1,
      "indent": ["error", 2]
    }
  }
  